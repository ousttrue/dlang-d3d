	"description": "A minimal D application.",
	"license": "proprietary",
	"name": "dlang-d3d",
	"dflags": [
		"-J=source"
	],
	"libs": [
		"user32",
		"d3d11",
		"d3dcompiler",
	],
	"buildTypes": {
		"debug": {
			"dflags": [
				"-mscrtlib=MSVCRTD",
				"-g",
				"-debug"
			],
			"libs": [
				"build/Debug/lib/imgui"
			]
		},
		"release": {
			"libs": [
				"build/Release/lib/imgui"
			],
			"dflags": [
				"-mscrtlib=msvcrt"
			]
		}
	}
}