	"copyright": "Copyright Â© 2019, ousttrue",
	"description": "A minimal D application.",
	"license": "proprietary",
	"name": "d3d11_imgui",
	"targetType": "executable",
	"dflags": [
		"-J=source"
	],
	"subPackages": [
		"../dlang_generated/"
	],
	"dependencies": {
		"d3d11_imgui:dlang_generated": "*"
	},
	"libs": [
		"user32",
		"d3d11",
		"d3dcompiler"
	],
	"buildTypes": {
		"debug": {
			"dflags": [
				"-mscrtlib=MSVCRTD",
				"-g",
				"-debug"
			],
			"libs": [
				"../build/Debug/lib/imgui"
			]
		},
		"release": {
			"libs": [
				"../build/Release/lib/imgui"
			],
			"dflags": [
				"-mscrtlib=msvcrt"
			]
		}
	}
}